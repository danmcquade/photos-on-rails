<h1>All Users</h1>
<% @users.each do |u| %>
<p><%= link_to u.username, u %></p>
<div class="index-gallery">
<% u.photos.each do |p| %>
  <!-- <%= link_to image_tag(p.url, :class => "main-img"), [u, p] %> -->
  <div class="index-gallery-img" style='background-image: url("<%= p.url %>")'><%= link_to ' ', [u, p] %></div>
<% end %>
</div>
<% end %>
